<!DOCTYPE html>
<html>
<head>
  <title>DinnerDash20</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <div class="row">
      <div class="col-xs-8" id="restaurant-name">
        <h1>Sheazonne's Italian Restaurant</h1>
      </div>
      <div class="col-xs-4" id="login-register">
        <h3><%= link_to "Login", "#" %>
          <%= link_to "Register", "#" %></h3>
      </div>
    </div>
  </header>
<%= yield %>

</body>
</html>
